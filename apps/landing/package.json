    "lucide-react": "^0.511.0"
  },
  "devDependencies": {
    "@kairo/webapp": "workspace:*",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "typescript": "~5.9.3"
  }
}
